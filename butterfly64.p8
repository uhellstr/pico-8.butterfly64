pico-8 cartridge // http://www.pico-8.com
version 8
__lua__

-- buttefly64
-- by uhellstr 2016

-- text_effects (scan_text,put_sintext) by saccharine
-- music borrowed from butterfly.p8 included in the demos and all
-- credits for music goes to zip

--[[

  butterfly64 is a convertion from a basic demo i
  wrote for the commodore 64 basic in 2010 to pico-8.

  the original c64 demo can be seen on youtube at
  https://www.youtube.com/watch?v=2moywbhdr94

  Instructions:

  Use X button to restart the demo sequence

]]

t_arr={}
color=1
cols=40
rows=25
timer=0
angle=0
dangle=0.0240
pic=1
w = 3
i = 1
j = 0


--[[
  scans text strings and get the colors of indivual pixels
]]
function scan_text(text)
  local x, y
  cls()
  scan={}
  print(text,0,0,1)
  for x=0,(#text)*4 do
    scan[x]={}
    for y=0,6 do
      scan[x][y]=pget(x,y)
    end
  end
  cls()
  return scan
end

--[[
  converts text pixels into sinpixels
]]
function put_sintext(text,x,y,c)
  local i,j
  for i=0,#text do
    for j=0,6 do
      a=sin(angle)*6
      if text[i][j]==1 then
        pset(i*2+x,j*2+y+a,(i+j)%3+c)
      end
    end
    angle+=dangle
    if (angle>1) angle=0
  end
end

--[[
  Generates a table of scrolling text
]]
function gen_arr()
  add(t_arr,scan_text("butterfly64"))
  add(t_arr,scan_text("     by    "))
  add(t_arr,scan_text("  uhellstr "))
  add(t_arr,scan_text("  20160707 "))
  add(t_arr,scan_text("   a demo  "))
  add(t_arr,scan_text(" converted "))
  add(t_arr,scan_text("  from c64 "))
  add(t_arr,scan_text("...basic..."))
  add(t_arr,scan_text(" to pico-8!"))
  add(t_arr,scan_text(" thank's to"))
  add(t_arr,scan_text("saccharine!"))
  add(t_arr,scan_text("  for text "))
  add(t_arr,scan_text("  effects! "))
  add(t_arr,scan_text("  zep for  "))
  add(t_arr,scan_text(" the music "))
end

--[[
 plot puts a circfil graphics on a
 madeup 40x25 character
 e.g simulate a c&4 text screen
]]
function plot(x,y,c)
  local x1, y1
  if x == 1 then
    x1 = 3
  else
    x1 = 3 * x
  end
  if y == 1 then
    y1 = 3
  else
    y1 = 3 * y
  end
  if x > cols then
    x1 = 3 * cols
  end
  if y > rows then
    y1 = 3 * rows
  end
  circfill(x1,y1,1,c)
  --rectfill(x1,y1,x1+3,y1+3,c)
end

--[[
  butterfly64 is where the magic occurs
]]
function butterflyc64(w,i,j)
  k = i + j
  c = flr(rnd(15))
  plot(i,k,c)
  plot(k,i,c)
  plot(cols-i,rows-k,c)
  plot(cols-k,rows-i,c)
  plot(k,rows-i,c)
  plot(cols-i,k,c)
  plot(i,rows-k,c)
  plot(cols-k,i,c)
end

--[[
  called when pressing the X button
]]
function start_over()
  local x1,y1
  for x1=1,cols
  do
    for y1=1,rows
    do
      plot(x1,y1,0)
    end
  end
  w = 3
  i = 1
  j = 0
end

--[[
  timer_ function for the scrolling text
]]
function timer_()
  timer = timer + 1
  if timer > 100 then
    timer = 0
    pic = pic + 1
    color = color + 4
    if pic > #t_arr then
      pic = 1
      color = 1
    end
    txt = t_arr[pic]
  end
end

--[[
  Demo loop starts hers
]]
function _init()
  cls()
  gen_arr()
  txt=t_arr[1]
  music(0)
end

function _update()
  timer_()
  w +=1
  i +=1
  j +=1
  if w > 50 then
    w = 3
  end
  if i > 12 then
    i = 1
  end
  if j > 12 then
    j = 0
  end
  if btn(5) then
    start_over()
  end
end

function _draw()
  rectfill(0,90,120,120,0)
  butterflyc64(w,i,j)
  put_sintext(txt,16,105,color)
end

